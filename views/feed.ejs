<div class="container">
  <div class="page-header" style="padding: 0; margin: 0; margin-bottom: 20px;">
    <h1 style="margin-top: 10px;">
      Job Feed
      <small>In case you missed something</small>
    </h1>
  </div>

  <section ng-controller="FeedCtrl">
    <div>
      <button class="btn btn-success" ng-click="createNewFeed()">Create demo feed</button>
    </div>
    <div ng-repeat="entry in feedEntries | orderBy:'+'" class="animation feed-entry">
      <span class="pull-right text-muted" am-time-ago="entry.createdAt"></span>
      <i class="fa fa-{{entry.icon}} text-muted"></i>
        {{entry.title}}
      <small class="meta text-muted">
        {{entry.description}}
      </small>
    </div>
  </section>
</div>
